{
  "manifest_version": 2,
  "name": "Delete Elements",
  "version": "dev: 1.0",

  "background": {
    "scripts": [ "background.js" ]
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ],

  "icons": {
    "48": "icons/delete_element_48.png"
  },

  "permissions": [ "contextMenus" ]
}