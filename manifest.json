{
  "manifest_version": 2,
  "name": "Delete Elements",
  "version": "0.1pre",

  "background": {
    "scripts": [ "background.js" ]
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ],

  "icons": {
    "48": "icons/delete_element_48.png"
  },

  "permissions": [ "contextMenus" ],
  
  "commands": {
    "undo": {
      "suggested_key": { "default": "Ctrl+Alt+Z" },
      "description": "Undos deletion of element"
    }
  }
}